{
  "name": "electon-react-boilerplate",
  "version": "0.0.1",
  "description": "Electron React Boilerplate",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all --parallel --race _build:watch _serve start:electron",
    "start:prod": "cross-env NODE_ENV=production npm-run-all --parallel --race _build:watch _serve start:electron",
    "start:electron": "electron .",
    "build": "webpack",
    "_build:watch": "npm run build -- --watch",
    "_serve": "http-server -p 8081 -c-1 dist --silent",
    "lint": "eslint **/ --ext .jsx,.js --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vutran/electron-webpack-boilerplate.git"
  },
  "keywords": [
    "electron",
    "webpack",
    "react",
    "boilerplate",
    "node"
  ],
  "author": "Vu Tran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vutran/electron-react-boilerplate/issues"
  },
  "homepage": "https://github.com/vutran/electron-react-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "0.0.5",
    "css-loader": "^0.23.0",
    "electron": "^1.6.8",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "http-server": "^0.10.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.1",
    "postcss": "^5.0.13",
    "postcss-loader": "^0.8.0",
    "sass-loader": "^4.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "electron-prebuilt": "^1.4.13",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "html-webpack-plugin": "^2.28.0",
    "prop-types": "^15.5.10",
    "react": "15.5.4",
    "react-color": "^2.11.7",
    "react-dom": "^15.5.4",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.5",
    "reactcss": "1.2.0",
    "redux": "^3.6.0",
    "source-map-loader": "^0.2.1"
  }
}
