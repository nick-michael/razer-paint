{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "rules": {
        "indent": ["error", 4, {"SwitchCase": 1}],
        "jsx-a11y/no-static-element-interactions": 0,
        "react/jsx-indent": [2, 4],
        "no-unused-expressions": [2, { "allowShortCircuit": true }],
        "linebreak-style": "off"
    }
}